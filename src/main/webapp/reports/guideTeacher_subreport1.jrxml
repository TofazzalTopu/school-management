	<parameter name="academic_year" class="java.lang.String"/>
	<parameter name="studentids" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT st.studentid studentid, st.name studentName, cls.name className, sec.name sectionName, st.roll_no, reg.mobile
            from student st
            inner join registration reg on st.registration_id = reg.id
            inner join section sec on st.section_id = sec.id
            inner join class_name cls on sec.class_name_id = cls.id
            where st.academic_year = $P{academic_year} and st.student_status = "NEW"  and st.studentid in ($P!{studentids})
            order by cls.sort_position asc, sec.sort_order asc, st.roll_no asc;]]>
	</queryString>
